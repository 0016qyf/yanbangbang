<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyb.yanbangbang.mapper.ResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dyb.yanbangbang.entity.Resource">
        <id column="resource_id" property="resourceId" />
        <result column="uploader_id" property="uploaderId" />
        <result column="type" property="type" />
        <result column="resource_name" property="resourceName" />
        <result column="resource_summary" property="resourceSummary" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        resource_id, uploader_id, type, resource_name, resource_summary, create_time, update_time
    </sql>

</mapper>
