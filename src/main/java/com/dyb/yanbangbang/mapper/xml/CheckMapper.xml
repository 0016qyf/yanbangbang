<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dyb.yanbangbang.mapper.CheckMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.dyb.yanbangbang.entity.Check">
        <id column="check_id" property="checkId" />
        <result column="material_id" property="materialId" />
        <result column="material_name" property="materialName" />
        <result column="checker_id" property="checkerId" />
        <result column="checker_name" property="checkerName" />
        <result column="state" property="state" />
        <result column="reason" property="reason" />
        <result column="check_time" property="checkTime" />
        <result column="pass_time" property="passTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        check_id, material_id, material_name, checker_id, checker_name, state, reason, check_time, pass_time
    </sql>

</mapper>
